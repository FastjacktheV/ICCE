%%
// in 'houseboat' and 'household' the first match is used since both are same length

	// matches houseboat and household
(houseboat|household)[\n \t]		    	std::cout << "specialword:" <<  matched() << '\n';

	// matches words everywhere
[^\n \t]+					std::cout << "word:" <<  matched() <<  '\n';
.				
